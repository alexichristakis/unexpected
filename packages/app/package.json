{
  "name": "@unexpected/app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator 'iPhone 11'",
    "start": "react-native start",
    "deploy": "appcenter codepush release-react -a alexi/unexpected -t '0.0.2'",
    "deploy-production": "appcenter codepush release-react -a alexi/unexpected -t '0.0.2' -d Production",
    "check-types": "tsc -p . --noEmit --skipLibCheck",
    "lint": "tslint -c tslint.json 'src/**/*.ts*' --fix",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/masked-view": "^0.1.5",
    "@react-native-community/netinfo": "^5.2.0",
    "@react-navigation/bottom-tabs": "^5.0.0-alpha.34",
    "@react-navigation/core": "^5.0.0-alpha.32",
    "@react-navigation/native": "^5.0.0-alpha.24",
    "@react-navigation/native-stack": "^5.0.0-alpha.24",
    "@svgr/core": "^5.0.1",
    "axios": "^0.19.1",
    "formik": "^2.0.8",
    "immer": "^5.2.1",
    "react": "16.12.0",
    "react-native": "0.61.5",
    "react-native-camera": "^3.15.1",
    "react-native-circular-progress": "^1.3.4",
    "react-native-code-push": "^6.0.0",
    "react-native-contacts": "^5.0.6",
    "react-native-fs": "^2.16.2",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-haptic-feedback": "^1.8.2",
    "react-native-image-resizer": "^1.1.0",
    "react-native-notifications": "3.0.0-beta.1",
    "react-native-permissions": "^2.0.8",
    "react-native-reanimated": "^1.4.0",
    "react-native-redash": "^9.5.4",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "^2.0.0-alpha.23",
    "react-native-svg": "^9.13.6",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-batched-actions": "^0.4.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn check-types && yarn test"
    }
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "setupFilesAfterEnv": [
      "./__tests__/init_jest.tsx"
    ],
    "testPathIgnorePatterns": [
      "./__tests__/utils.tsx",
      "./__tests__/init_jest.tsx"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
