{
  "name": "@unexpected/server",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "npm run-script tsc",
    "test": "mocha --require ts-node/register --check-leaks ./test/**/*.ts --recursive",
    "coverage": "test-cov && test-remap",
    "dev": "mongod --config /usr/local/etc/mongod.conf & source .env && nodemon --watch '**/*.ts' --ignore 'node_modules/**/*' --exec ts-node src/index.ts",
    "tsc": "tsc --project tsconfig.json --skipLibCheck",
    "tsc:w": "tsc -w",
    "start": "node dist/index.js",
    "deploy": "yarn tsc && gcloud app deploy"
  },
  "files": [
    "src/"
  ],
  "dependencies": {
    "@google-cloud/storage": "^4.3.1",
    "@sentry/node": "^5.12.3",
    "@slack/web-api": "^5.7.0",
    "@tsed/common": "^5.42.1",
    "@tsed/core": "^5.42.1",
    "@tsed/di": "^5.42.1",
    "@tsed/mongoose": "^5.42.1",
    "@tsed/multipartfiles": "^5.42.1",
    "agenda": "^3.0.0",
    "apn": "^2.2.0",
    "atob": "^2.1.2",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "jimp": "^0.9.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "mongoose": "^5.9.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "twilio": "^3.39.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.6.2"
  }
}
