{
  "name": "@unexpected/server",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "npm run-script tsc",
    "test": "mocha --require ts-node/register --check-leaks ./test/**/*.ts --recursive",
    "coverage": "test-cov && test-remap",
    "mongo": "mongod --config mongo/mongo.conf &",
    "nodemon": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "yarn mongo && yarn nodemon",
    "tsc": "tsc --project tsconfig.json --skipLibCheck",
    "tsc:w": "tsc -w",
    "start": "node dist/index.js",
    "deploy": "yarn tsc && gcloud app deploy"
  },
  "files": [
    "src/"
  ],
  "dependencies": {
    "@google-cloud/storage": "^5.0.0",
    "@sentry/node": "^5.15.5",
    "@slack/web-api": "^5.8.1",
    "@tsed/common": "^5.53.1",
    "@tsed/core": "^5.53.1",
    "@tsed/di": "^5.53.1",
    "@tsed/mongoose": "^5.53.1",
    "@tsed/multipartfiles": "^5.53.1",
    "agenda": "^3.1.0",
    "apn": "^2.2.0",
    "atob": "^2.1.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "express": "~5.0.0-alpha.8",
    "jimp": "^0.12.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.9.14",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "ts-httpexceptions": "^4.1.0",
    "twilio": "^3.43.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "supertest": "^5.0.0-0",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9"
  }
}
