    "@types/express-jwt": "^0.0.42",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/method-override": "^0.0.31",
    "@types/mongoose": "^5.5.20",
    "@types/morgan": "^1.7.36",
    "@types/stripe": "^6.31.13",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0"
  }
}
