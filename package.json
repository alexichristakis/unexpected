{
  "name": "unexpected-cloud",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "yarn tsc",
    "coverage": "test-cov && test-remap",
    "dev": "source .env && nodemon --watch '**/*.ts' --ignore 'node_modules/**/*' --exec ts-node src/index.ts",
