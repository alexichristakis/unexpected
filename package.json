    "@tsed/mongoose": "^5.32.1",
    "atob": "^2.1.2",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "jimp": "^0.6.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.1",
