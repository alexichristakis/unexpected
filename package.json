{
  "name": "unexpected",
  "private": true,
  "scripts": {
    "pods": " cd packages/app/ios && pod install",
    "types-cloud": "cd packages/server && yarn tsc",
    "types-app": "cd packages/app && yarn check-types",
    "types-global": "cd packages/global && yarn check-types",
    "types-web": "cd packages/web && yarn check-types",
    "check-types": "yarn types-cloud; yarn types-app; yarn types-web",
    "check-updates": "ncu -n && ncu -n --packageFile packages/app/package.json && ncu -n --packageFile packages/server/package.json",
    "lint": "tslint -c tslint.json 'packages/**/*.ts*' --exclude '**/node_modules/**/*' --fix"
  },
  "engines": {
    "node": "13.5.0"
  },
  "dependencies": {
    "@unexpected/app": "./packages/app",
    "@unexpected/server": "./packages/server",
    "@unexpected/global": "./packages/global"
  },
  "devDependencies": {
    "@tsed/testing": "^5.43.1",
    "@types/agenda": "^2.0.6",
    "@types/atob": "^2.1.2",
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.9",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/enzyme": "^3.10.5",
    "@types/express": "^4.17.2",
    "@types/express-jwt": "^0.0.42",
    "@types/jest": "^25.1.3",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/lodash": "^4.14.149",
    "@types/method-override": "^0.0.31",
    "@types/mocha": "^7.0.1",
    "@types/moment-timezone": "^0.5.12",
    "@types/mongoose": "^5.7.1",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.4.2",
    "@types/node": "13.7.4",
    "@types/react-dom": "16.9.5",
    "@types/react": "16.9.21",
    "@types/react-native": "^0.61.16",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "16.9.2",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.7",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-context": "^1.1.2",
    "enzyme-context-redux": "^2.0.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-enzyme": "^7.1.2",
    "lerna": "^3.20.2",
    "metro-react-native-babel-preset": "^0.58.0",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "patch-package": "^6.2.0",
    "react-dom": "^16.12.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.12.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.2",
    "utility-types": "^3.10.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn check-types"
    }
  }
}
